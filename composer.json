{
    "name": "ixocreate/cms",
    "description": "IXOCREATE CMS Package",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "require": {
        "php": "^7.1",
        "ixocreate/application": "dev-consolidate",
        "symfony/options-resolver": "^4.1",
        "symfony/routing": "^4.2.4",
        "thepixeldeveloper/sitemap": "^5.1"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Package\\Cms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\Cms\\": "tests/misc/",
            "Ixocreate\\Test\\Cms\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
