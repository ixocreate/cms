{
    "name": "ixocreate/cms-package",
    "description": "IXOCREATE CMS Package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE",
            "email": "opensource@ixocreate.com",
            "homepage": "https://www.ixocreate.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ixocreate/admin-package": "^0.3",
        "ixocreate/application": "^0.2",
        "ixocreate/cache-package": "^0.1",
        "symfony/options-resolver": "^4.1",
        "symfony/routing": "^4.2.4",
        "thepixeldeveloper/sitemap": "^5.1"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^2.0",
        "ixocreate/test": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Cms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\Cms\\": "tests/misc/",
            "Ixocreate\\Test\\Cms\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
